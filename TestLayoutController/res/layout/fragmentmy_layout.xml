<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/relativeLayoutFragmentMy"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ff00ff00" >
    <TextView
        android:id="@+id/textViewFragmentMy"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="FragmentMy" />

    <EditText
        android:id="@+id/editTextFragmentMy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:layout_below="@id/textViewFragmentMy" >

        <requestFocus />
    </EditText>

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_below="@id/editTextFragmentMy"
        android:background="#ff0000ff"
        android:src="@drawable/ic_launcher" />

</RelativeLayout>
